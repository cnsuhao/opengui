<Template name="tachometer" group="OpenGUIExt" type="Tachometer">
	<Property name="BGImagery" value="TachBG" />
	<Property name="NeedleImagery" value="TachNeedle" />
	
	<Property name="position" value="(0.59875 x0.465)" />
